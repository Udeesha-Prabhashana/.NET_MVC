@model List<Category>       //import list of category of model

<h1> Category list</h1>

<table class="table table-bordered table-striped">
    <thead>     

    <tr>
        <th>
            Category Name
        </th>
        <th>
            Display Order
        </th>

    </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOrder))        //Model is a above impoarted category model 
        {      
        <tr>
            <td> @obj.Name </td>                 
            <td> @obj.DisplayOrder</td>
        </tr>
        }
    </tbody>
</table>

